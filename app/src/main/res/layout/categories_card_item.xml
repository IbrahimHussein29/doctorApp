<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="@dimen/category_margin_end"
    app:cardCornerRadius="@dimen/radius_edit_text"
    android:elevation="0dp">

    <TextView
        android:id="@+id/category_type_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/secondary"
        android:fontFamily="@font/open_sans_semibold"
        android:paddingHorizontal="@dimen/category_horizontal_padding"
        android:paddingVertical="@dimen/category_vertical_padding"
        android:textColor="@color/white"
        android:textSize="@dimen/time_size"
        tools:text="@string/denteeth_text"
        />

</com.google.android.material.card.MaterialCardView>