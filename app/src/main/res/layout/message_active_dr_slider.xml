<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginEnd="28.28dp"
    android:id="@+id/online_dr">
    <ImageView
        android:id="@+id/online_dr_img"
        android:layout_width="@dimen/online_img_size"
        android:layout_height="@dimen/online_img_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/online_dr_1"
        />
    <ImageView
        android:id="@+id/online_status_img"
        android:layout_width="15dp"
        android:layout_height="15dp"
        tools:src="@drawable/ic_online"
        app:layout_constraintBottom_toBottomOf="@id/online_dr_img"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/ic_online_margin_start"
        android:layout_marginBottom="@dimen/ic_online_margin_bottom"

        />

</androidx.constraintlayout.widget.ConstraintLayout>